package com.if2c.harald.beans;

public class GoodsPromotion {
private long id;
private float priceReach;
private float priceDown;
private int discount;
private int rule;
private int type;
private int costBear;

public int getDiscount() {
	return discount;
}
public void setDiscount(int discount) {
	this.discount = discount;
}

public int getCostBear() {
	return costBear;
}
public void setCostBear(int costBear) {
	this.costBear = costBear;
}
public long getId() {
	return id;
}
public void setId(long id) {
	this.id = id;
}
public float getPriceReach() {
	return priceReach;
}
public void setPriceReach(float priceReach) {
	this.priceReach = priceReach;
}
public float getPriceDown() {
	return priceDown;
}
public void setPriceDown(float priceDown) {
	this.priceDown = priceDown;
}
public int getRule() {
	return rule;
}
public void setRule(int rule) {
	this.rule = rule;
}
public int getType() {
	return type;
}
public void setType(int type) {
	this.type = type;
}


}
