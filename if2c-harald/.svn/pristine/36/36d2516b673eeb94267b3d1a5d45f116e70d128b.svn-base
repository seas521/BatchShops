package com.if2c.harald.beans;

import java.util.Date;

public class AttributeCategoryValueRelation {
    private Integer id;

    private Integer attributeCategoryId;

    private Integer attributeValueId;

    private Integer position;

    private Integer status;

    private Date createdDate;
    private String  attributeNameName ;
    private String attributeValueValue ;
    private Long categoryId ;
    private Integer attributeId ;

    public Integer getId() {
        return id;
    }


    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getAttributeCategoryId() {
        return attributeCategoryId;
    }

    public void setAttributeCategoryId(Integer attributeCategoryId) {
        this.attributeCategoryId = attributeCategoryId;
    }

    public Integer getAttributeValueId() {
        return attributeValueId;
    }

    public void setAttributeValueId(Integer attributeValueId) {
        this.attributeValueId = attributeValueId;
    }

    public Integer getPosition() {
        return position;
    }

    public void setPosition(Integer position) {
        this.position = position;
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }

    public Date getCreatedDate() {
        return createdDate;
    }

    public void setCreatedDate(Date createdDate) {
        this.createdDate = createdDate;
    }

    public String getAttributeNameName() {
        return attributeNameName;
    }

    public void setAttributeNameName(String attributeNameName) {
        this.attributeNameName = attributeNameName;
    }

    public String getAttributeValueValue() {
        return attributeValueValue;
    }

    public void setAttributeValueValue(String attributeValueValue) {
        this.attributeValueValue = attributeValueValue;
    }


    public Integer getAttributeId() {
        return attributeId;
    }

    public void setAttributeId(Integer attributeId) {
        this.attributeId = attributeId;
    }

    public Long getCategoryId() {
        return categoryId;
    }

    public void setCategoryId(Long categoryId) {
        this.categoryId = categoryId;
    }
}