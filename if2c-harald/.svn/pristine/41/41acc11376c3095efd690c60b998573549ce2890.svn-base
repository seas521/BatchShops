package com.if2c.harald.beans;

import java.io.Serializable;
import java.util.Date;

public class Gacvr implements Serializable {
    private Long id;

    private Long goodsId;

    private Integer attributeCategoryValueRelationId;

    private Integer status;

    private Date createdDate;
    public Gacvr(){

    }

    public Gacvr(Long goodsId,Integer attributeCategoryValueRelationId,Integer status){
        this.goodsId = goodsId ;
        this.status = status ;
        this.attributeCategoryValueRelationId = attributeCategoryValueRelationId ;
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public Long getGoodsId() {
        return goodsId;
    }

    public void setGoodsId(Long goodsId) {
        this.goodsId = goodsId;
    }

    public Integer getAttributeCategoryValueRelationId() {
        return attributeCategoryValueRelationId;
    }

    public void setAttributeCategoryValueRelationId(Integer attributeCategoryValueRelationId) {
        this.attributeCategoryValueRelationId = attributeCategoryValueRelationId;
    }


    public Date getCreatedDate() {
        return createdDate;
    }

    public void setCreatedDate(Date createdDate) {
        this.createdDate = createdDate;
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }
}